[global]
ioengine=external:./phoenix_fio.so
bs=4k
iodepth=16
iodepth_batch_submit=16
iodepth_batch_complete_min=16
iodepth_batch_complete_max=16
direct=1
device_id=0
enable_nds=1
enable_iouring=1
group_reporting
numjobs=2
name=raw-randread
rw=randread


[job1]
filename=/dev/nvme0n1
size=1G
runtime=40
time_based=1